<%= render 'layouts/slide'%>
  <div>
    <button class= "btn btn-outline-primary btn-md" data-toggle="modal" data-target="#ModalAddInvoice">Add a new bill</button>
  </div>
<%= render 'homes/property2'%>

<%= render 'homes/finalize_property'%>
<%= render 'homes/builder'%>
<%= render 'homes/client'%>
<%#= render 'homes/enquirey'%>


<!-- Modal -->
<div class="modal fade" id="ModalAddInvoice" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Invoice</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="errors_invoice">
        </div>
        <%#= render "form" %>
		<%= render 'enquiries/form'%>
      </div>
    </div>
  </div>
</div>
